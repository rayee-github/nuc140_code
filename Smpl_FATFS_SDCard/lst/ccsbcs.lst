NWCHAR ff_convert (	/* Converted character, Returns zero on error */
N	WCHAR	src,	/* Character code to be converted */
N	UINT	dir		/* 0: Unicode to OEMCP, 1: OEMCP to Unicode */
N)
N{
N	WCHAR c;
N
N
N	if (src < 0x80) {	/* ASCII */
N		c = src;
N
N	} else {
N		if (dir) {		/* OEMCP to Unicode */
N			c = (src >= 0x100) ? 0 : Tbl[src - 0x80];
N
N		} else {		/* Unicode to OEMCP */
N			for (c = 0; c < 0x80; c++) {
N				if (src == Tbl[c]) break;
N			}
N			c = (c + 0x80) & 0xFF;
N		}
N	}
N
N	return c;
N}
