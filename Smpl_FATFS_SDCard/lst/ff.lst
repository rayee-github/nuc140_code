S/*-----------------------------------------------------------------------*/
S/* Get a string from the file                                            */
S/*-----------------------------------------------------------------------*/
STCHAR* f_gets (
S	TCHAR* buff,	/* Pointer to the string buffer to read */
S	int len,		/* Size of string buffer (characters) */
S	FIL* fil		/* Pointer to the file object */
S)
S{
S	int n = 0;
S	TCHAR c, *p = buff;
S	BYTE s[2];
S	UINT rc;
S
S
S	while (n < len - 1) {			/* Read bytes until buffer gets filled */
S		f_read(fil, s, 1, &rc);
S		if (rc != 1) break;			/* Break on EOF or error */
S		c = s[0];
S#if _LFN_UNICODE					/* Read a character in UTF-8 encoding */
S		if (c >= 0x80) {
S			if (c < 0xC0) continue;	/* Skip stray trailer */
S			if (c < 0xE0) {			/* Two-byte sequense */
S				f_read(fil, s, 1, &rc);
S				if (rc != 1) break;
S				c = ((c & 0x1F) << 6) | (s[0] & 0x3F);
S				if (c < 0x80) c = '?';
S			} else {
S				if (c < 0xF0) {		/* Three-byte sequense */
S					f_read(fil, s, 2, &rc);
S					if (rc != 2) break;
S					c = (c << 12) | ((s[0] & 0x3F) << 6) | (s[1] & 0x3F);
S					if (c < 0x800) c = '?';
S				} else {			/* Reject four-byte sequense */
S					c = '?';
S				}
S			}
S		}
S#endif
S#if _USE_STRFUNC >= 2
S		if (c == '\r') continue;	/* Strip '\r' */
S#endif
S		*p++ = c;
S		n++;
S		if (c == '\n') break;		/* Break on EOL */
S	}
S	*p = 0;
