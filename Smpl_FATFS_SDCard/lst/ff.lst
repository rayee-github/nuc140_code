S		dj->fs->flsem[i].clu = dj->sclust;
S		dj->fs->flsem[i].idx = dj->index;
S	}
S
S	if (acc && dj->fs->flsem[i].ctr) return 0;	/* Access violation (int err) */
S
S	dj->fs->flsem[i].ctr = acc ? 0x100 : dj->fs->flsem[i].ctr + 1;	/* Set semaphore value */
S
S	return i + 1;
S}
