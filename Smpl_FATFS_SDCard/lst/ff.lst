Nstatic
NFRESULT dir_next (	/* FR_OK:Succeeded, FR_NO_FILE:End of table, FR_DENIED:EOT and could not stretch */
N	DIR *dj,		/* Pointer to directory object */
N	int stretch		/* 0: Do not stretch table, 1: Stretch table if needed */
N)
