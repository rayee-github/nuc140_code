Sstatic
SFRESULT dec_lock (	/* Decrement file open counter */
S	FATFS* fs,		/* File system object */
S	UINT i			/* Semaphore index */
S)
S{
S	WORD n;
S	FRESULT res;
S
S
S	if (--i < _FS_SHARE) {
S		n = fs->flsem[i].ctr;
S		if (n >= 0x100) n = 0;
S		if (n) n--;
