S/*-----------------------------------------------------------------------*/
S/* Put a formatted string to the file                                    */
S/*-----------------------------------------------------------------------*/
Sint f_printf (
S	FIL* fil,			/* Pointer to the file object */
S	const TCHAR* str,	/* Pointer to the format string */
S	...					/* Optional arguments... */
S)
S{
S	va_list arp;
S	BYTE f, r;
S	UINT i, w;
S	ULONG val;
S	TCHAR c, d, s[16];
S	int res, cc;
S
S
S	va_start(arp, str);
S
S	for (cc = res = 0; cc != EOF; res += cc) {
S		c = *str++;
S		if (c == 0) break;			/* End of string */
S		if (c != '%') {				/* Non escape character */
S			cc = f_putc(c, fil);
S			if (cc != EOF) cc = 1;
S			continue;
S		}
S		w = f = 0;
S		c = *str++;
S		if (c == '0') {				/* Flag: '0' padding */
S			f = 1; c = *str++;
S		}
S		while (IsDigit(c)) {		/* Precision */
S			w = w * 10 + c - '0';
S			c = *str++;
S		}
S		if (c == 'l' || c == 'L') {	/* Prefix: Size is long int */
S			f |= 2; c = *str++;
S		}
S		if (!c) break;
S		d = c;
S		if (IsLower(d)) d -= 0x20;
S		switch (d) {				/* Type is... */
S		case 'S' :					/* String */
S			cc = f_puts(va_arg(arp, TCHAR*), fil); continue;
S		case 'C' :					/* Character */
S			cc = f_putc((TCHAR)va_arg(arp, int), fil); continue;
S		case 'B' :					/* Binary */
S			r = 2; break;
S		case 'O' :					/* Octal */
S			r = 8; break;
S		case 'D' :					/* Signed decimal */
S		case 'U' :					/* Unsigned decimal */
S			r = 10; break;
S		case 'X' :					/* Hexdecimal */
S			r = 16; break;
S		default:					/* Unknown */
S			cc = f_putc(c, fil); continue;
S		}
S
S		/* Get an argument */
S		val = (f & 2) ? va_arg(arp, long) : ((d == 'D') ? (long)va_arg(arp, int) : va_arg(arp, unsigned int));
S		if (d == 'D' && (val & 0x80000000)) {
S			val = 0 - val;
S			f |= 4;
S		}
S		/* Put it in numeral string */
S		i = 0;
S		do {
S			d = (TCHAR)(val % r); val /= r;
S			if (d > 9) {
S				d += 7;
S				if (c == 'x') d += 0x20;
S			}
S			s[i++] = d + '0';
S		} while (val && i < sizeof(s) / sizeof(s[0]));
S		if (f & 4) s[i++] = '-';
S		cc = 0;
S		while (i < w-- && cc != EOF) {
S			cc = f_putc((TCHAR)((f & 1) ? '0' : ' '), fil);
S			res++;
S		}
S		do {
S			cc = f_putc(s[--i], fil); 
S			res++;
S		} while (i && cc != EOF);
S		if (cc != EOF) cc = 0;
S	}
S
S	va_end(arp);
S	return (cc == EOF) ? cc : res;
S}
