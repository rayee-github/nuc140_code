S/*-----------------------------------------------------------------------*/
S/* Put a string to the file                                              */
S/*-----------------------------------------------------------------------*/
Sint f_puts (
S	const TCHAR* str,	/* Pointer to the string to be output */
S	FIL* fil			/* Pointer to the file object */
S)
S{
S	int n;
S
S
S	for (n = 0; *str; str++, n++) {
S		if (f_putc(*str, fil) == EOF) return EOF;
S	}
S	return n;
S}
